curl -v --fail-with-body -k \
  -H "Content-Type: application/x-www-form-urlencoded" \
  -d "grant_type=client_credentials" \
  -d "client_id=<YOUR_STG_CLIENT_ID>" \
  -d "client_secret=<YOUR_STG_CLIENT_SECRET>" \
  "https://robotic-auth.staging.echonet/auth/realms/master/protocol/openid-connect/token"